{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "backend",
      "image": "BACKEND_REPO_URL:TAG",
      "essential": true,
      "memory": 1024,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "mountPoints": [
        {
          "sourceVolume": "awseb-logs-backend",
          "containerPath": "/var/log/falco-new"
        }
      ]
    },
    {
      "name": "celeryworker",
      "image": "CELERY_WORKER_REPO_URL:TAG",
      "essential": true,
      "memory": 1024,
      "mountPoints": [
        {
          "sourceVolume": "awseb-logs-celeryworker",
          "containerPath": "/var/log/falco-new-celeryworker"
        }
      ]
    },
    {
      "name": "celerybeat",
      "image": "CELERY_BEAT_REPO_URL:TAG",
      "essential": true,
      "memory": 256,
      "mountPoints": [
        {
          "sourceVolume": "awseb-logs-celerybeat",
          "containerPath": "/var/log/falco-new-celerybeat"
        }
      ]
    },
    {
      "name": "static",
      "image": "STATIC_REPO_URL:TAG",
      "essential": true,
      "memory": 256,
      "portMappings": [
        {
          "hostPort": 81,
          "containerPort": 80
        }
      ]
    }
  ]
}
