{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "backend",
      "image": "BACKEND_REPO_URL:TAG",
      "essential": true,
      "memory": 1024,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "mountPoints": [
        {
          "sourceVolume": "awseb-logs-backend",
          "containerPath": "/var/log/falco"
        }
      ]
    },
    {
      "name": "celery_worker",
      "image": "CELEREY_WORKER_REPO_URL:TAG",
      "essential": true,
      "memory": 1024,
      "mountPoints": [
        {
          "sourceVolume": "awseb-logs-backend",
          "containerPath": "/var/log/falco-celery-worker"
        }
      ]
    },
    {
      "name": "static",
      "image": "STATIC_REPO_URL:TAG",
      "essential": true,
      "memory": 256,
      "portMappings": [
        {
          "hostPort": 81,
          "containerPort": 80
        }
      ]
    }
  ]
}
